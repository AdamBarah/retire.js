{
  "manifest_version": 2,
  "name": "retire.js",
  "description": "Scanning website for vulernable js libraries. Icon by http://www.studiomx.eu/",
  "version": "1.0",
  "icons": {
  	"16":"icons/icon16.png",
  	"48":"icons/icon48.png",
  	"128":"icons/icon128.png"
  },
  "author" : "Erlend Oftedal",

  "permissions": [
	"<all_urls>",
	"webRequest",
	"tabs"
  ],
  "browser_action": {
    "default_icon" : "icons/icon48.png",
    "default_name" : "retire.js"
  },
  "background": {
    "scripts"	: ["js/sha1.js", "js/retire.js", "js/plugin.js"],
    "persistent": true
  },
  "content_scripts" : [{
  	"matches"	: ["<all_urls>"],
  	"js"		: ["js/log.js"]
  }]
}